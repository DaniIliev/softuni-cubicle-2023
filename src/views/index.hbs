<main>
  <h1>Browser</h1>
  <form action="" method="">
    <input type="text" class="search" name="search" placeholder="Search..."value = "{{search}}">
    <input type="number" name="from" class="difficulty" placeholder="Difficulty from..." value ="{{difficultyLevelFrom}}">
    <span>-</span>
    <input type="number" name="to" class="difficulty" placeholder="Difficulty to..." value ="{{difficultyLevelTo}}">
    <input type="submit" value="search">
  </form>

  {{#each cubes}}
  {{>cube}}
  {{else}}
  <p>There are no cubes yet!</p>
  {{/each}}
</main>